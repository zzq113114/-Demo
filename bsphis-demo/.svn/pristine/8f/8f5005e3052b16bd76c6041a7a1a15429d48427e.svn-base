<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SystemUsers" table="SystemUsers">
		<id name="userId" type="java.lang.String" length="16">
			<column name="userId" />
			<generator class="assigned" />
		</id>
		<property name="logonName" type="java.lang.String" length="50" />
		<property name="password" type="java.lang.String" length="50" />
		<property name="userName" type="java.lang.String" length="100" />
		<property name="gender" type="java.lang.String" length="1" />
		<property name="jobTitle" type="java.lang.String" length="50" />
		<property name="manaUnitId" type="java.lang.String" length="20" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="pyCode" type="java.lang.String" length="50" />
		<property name="remark" type="java.lang.String" length="200" />
		<property name="mapSign" type="java.lang.String" length="10" />
		<property name="regionCode" type="java.lang.String" length="25" />
		<property name="regionCode_text" type="java.lang.String" length="200" />
		<property name="userVersion" type="timestamp" />
		<property name="lastModifyDate" type="date" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>