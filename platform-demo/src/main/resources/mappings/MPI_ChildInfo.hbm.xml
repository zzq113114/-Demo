<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="MPI_ChildInfo" table="MPI_ChildInfo">
		<id name="childEmpiId" type="java.lang.String" length="32">
			<column name="childEmpiId" />
			<generator class="assigned" />
		</id>
		
		<property name="relativeEmpiId" type="java.lang.String"
			length="32" />
		<property name="relativeName" type="java.lang.String"
			length="20" />
		<property name="relativeIdCard" type="java.lang.String"
			length="20" />
		<!-- 
		<property name="regionCode" type="java.lang.String" length="21" />
		<property name="manaDoctorId" type="java.lang.String" length="20" />
		 -->
		<property name="vaccinateCardNo" type="java.lang.String"
			length="20" />
		<property name="certificateNo" type="java.lang.String" length="10" />
	</class>
</hibernate-mapping>