<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SYS_AppLayout" table="SYS_AppLayout">
		<id name="id" type="java.lang.Integer" length="30">
			<column name="id" />
			<generator class="sequence">
            	<param name="sequence">SEQ_SYS_AppLayout</param>
           	</generator>
		</id>
		<property name="appId" type="java.lang.String" length="50" />
		<property name="userId" type="java.lang.String" length="30" />
		<property name="pageCount" type="java.lang.Integer"  />
	</class>
</hibernate-mapping>