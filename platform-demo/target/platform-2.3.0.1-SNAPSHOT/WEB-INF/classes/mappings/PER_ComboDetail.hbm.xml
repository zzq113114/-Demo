<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.per.schemas.PER_ComboDetail" table="PER_ComboDetail">
		<id name="id" type="java.lang.String">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="comboId" type="java.lang.String" length="16"/>
		<property name="projectOfficeId" type="java.lang.String" length="50"/>
		<property name="itemId" type="java.lang.String" length="2000"/>
		<property name="createUnit" type="java.lang.String" length="20"/>
   	 	<property name="createUser" type="java.lang.String" length="20"/>
    	<property name="createDate" type="timestamp"/>
    	<property name="lastModifyUnit" type="java.lang.String" length="20"/>
    	<property name="lastModifyDate" type="timestamp"/>
    	<property name="lastModifyUser" type="java.lang.String" length="20"/>
	</class>
</hibernate-mapping>