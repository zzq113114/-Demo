<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="SYS_USERS" table="SYS_USERS">
	    <id name="userId" type="java.lang.String" length="30">
			<column name="userId" />
			<generator class="assigned" />
		</id>
		<property name="loginName" type="java.lang.String" length="100" />
		<property name="password" type="java.lang.String" length="100" />
		<property name="userName" type="java.lang.String" length="100" />
		<property name="gender" type="java.lang.String" length="1" />
		<property name="status" type="java.lang.String" length="1" />
		<property name="pyCode" type="java.lang.String" length="100" />
		<property name="remark" type="java.lang.String" length="255" />
		<property name="mapSign" type="java.lang.String" length="10" />
		<property name="lastModifyDate" type="date" />
		<property name="lastModifyUser" type="java.lang.String" length="20" />
		<property name="lastModifyUnit" type="java.lang.String" length="20" />
		<property name="userVersion" type="java.lang.String" length="20" />
	</class>
</hibernate-mapping>