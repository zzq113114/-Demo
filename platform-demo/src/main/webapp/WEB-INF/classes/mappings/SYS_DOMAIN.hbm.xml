<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="SYS_DOMAIN" table="SYS_DOMAIN">
			<id name="ID" type="java.lang.String">
            	<column name="ID"/>
            	<generator class="assigned" />
        	</id>
        	<property name="DomainName" type="java.lang.String" length="50" />
        	<property name="REMARK" type="java.lang.String" length="200" />
        	<property name="Status" type="java.lang.String" length="2" />
        	<property name="DomainUrl" type="java.lang.String" length="50" />
</class>
</hibernate-mapping>