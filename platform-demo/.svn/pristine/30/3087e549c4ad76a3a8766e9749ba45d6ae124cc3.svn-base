<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class entity-name="chis.application.mpm.schemas.MPM_MasterplateMaintain" table="MPM_MasterplateMaintain">
			<id name="masterplateId" type="java.lang.String">
            	<column name="masterplateId"/>
            	<generator class="assigned" />
        	</id>
        	<property name="masterplateName" type="java.lang.String" length="100" />
        	<property name="manaUnitId" type="java.lang.String" length="20" />
        	<property name="year" type="java.lang.Integer" length="4" />
        	<property name="masterplateType" type="java.lang.String"  />
        	<property name="masterplateField" type="java.lang.String" length="50" />
        	<property name="inputUser" type="java.lang.String" length="20" />
        	<property name="inputUnit" type="java.lang.String" length="20" />
        	<property name="inputDate" type="date"  />
        	<property name="lastModifyUser" type="java.lang.String" length="20" />
        	<property name="lastModifyUnit" type="java.lang.String" length="20" />
        	<property name="lastModifyDate" type="date"  />
        	<property name="masterplateTypezb" type="java.lang.String" length="2" />
        	<property name="dateType" type="java.lang.String" length="2" />
        	<property name="whmb" type="java.lang.String" length="2" />
</class>
</hibernate-mapping>