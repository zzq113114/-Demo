<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="chis.application.per.schemas.PER_CheckupDict" table="PER_CheckupDict">
		<id name="checkupProjectId" type="java.lang.String"
			length="16">
			<column name="checkupProjectId" />
			<generator class="assigned" />
		</id>
		<property name="checkupProjectName" type="java.lang.String"
			length="60" />
		<property name="checkupUnit" type="java.lang.String"
			length="10" />
		<property name="referenceUpper" type="java.lang.Double"
			length="10" />
		<property name="referenceLower" type="java.lang.Double"
			length="10" />
		<property name="projectOfficeCode" type="java.lang.String"
			length="16" />
		<property name="projectOffice" type="java.lang.String"
			length="16" />
		<property name="projectType" type="java.lang.String" length="2" />
		<property name="orderNo" type="java.lang.Integer" length="2" />
		<property name="projectClass" type="java.lang.String"
			length="2" />
		<property name="checkupDic" type="java.lang.String"
			length="20" />
		<property name="memo" type="java.lang.String" length="100" />
	</class>
</hibernate-mapping>