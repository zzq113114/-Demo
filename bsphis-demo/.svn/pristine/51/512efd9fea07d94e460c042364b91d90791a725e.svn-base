@CHARSET "UTF-8";
.x-debug-view{
	background:black;
	height:100%;
	overflow:auto;
	cursor: pointer;
}
.x-debug-item{
	align: center;
	font-size:12px;
	color:white;
	padding:3px;
	padding-left:5px;
}
.x-debug-item span{
	color:yellow;
	font-size:13px;
	padding-left:58px;
}
