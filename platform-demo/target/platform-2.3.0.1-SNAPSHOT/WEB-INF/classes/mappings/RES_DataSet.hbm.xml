<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="RES_DataSet" table="RES_DataSet">
		<id name="DataSetId" type="java.lang.Integer">
	    	<column name="DataSetId"/>
			<generator class="sequence">
            	<param name="sequence">SEQ_RES_DataSet</param>
           	</generator>
		</id>
		<property name="DataStandardId" type="java.lang.String" length="16" />
		<property name="StandardIdentify" type="java.lang.String" length="50" />
		<property name="CustomIdentify" type="java.lang.String" length="50" />
		<property name="DName" type="java.lang.String" length="50" />
		<property name="Catalog" type="java.lang.Integer"  />
		<property name="DComment" type="java.lang.String" length="500" />
		<property name="Status" type="java.lang.Integer"  />
		<property name="DataSetMapping" type="java.lang.String" length="4000" />
		<property name="Parent" type="java.lang.String" length="50" />
		<property name="Foreignkey" type="java.lang.String" length="50" />
	</class>
</hibernate-mapping>